package models;

public enum EmployeeVariable {
    ID("personId"), FIRST_NAME("firstName"), LAST_NAME("lastName"), DOCUMENT_TYPE("Tipo documento"),
    DOCUMENT_NUMBER("Numero documento"), BIRTHDATE("dateOfBirthUTC"), HEIGHT("heightMeters"), AGE("algo_age");

    private String jsonName;

    EmployeeVariable(String jsonName){
        this.jsonName = jsonName;
    }

    public String getJsonName(){
        return jsonName;
    }
}